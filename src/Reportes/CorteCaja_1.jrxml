	<subDataset name="DS_Empleado" uuid="d8a22e19-7a23-4e43-a105-679bfce1ed5d">
		<queryString language="SQL">
			<![CDATA[SELECT
     SUM(total),
     sales.`id_vendedor`,
     sales.`nombre_vendedor`
FROM
     `sales` sales
WHERE
     CAST(register_date as date) LIKE '%$F{register_date}%'
GROUP BY
     total
     DESC
     LIMIT 1]]>
		</queryString>
		<field name="id_vendedor" class="java.lang.Integer">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="nombre_vendedor" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="SUM(total)" class="java.lang.Double"/>
		<group name="id_vendedor">
			<groupExpression><![CDATA[$F{id_vendedor}]]></groupExpression>
		</group>
		<group name="nombre_vendedor">
			<groupExpression><![CDATA[$F{nombre_vendedor}]]></groupExpression>
		</group>
		<group name="SUM(total)">
			<groupExpression><![CDATA[$F{SUM(total)}]]></groupExpression>
		</group>
	</subDataset>
	<parameter name="f1" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="f2" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT CAST(sl.register_date as Date), sl.id_sale,ul.firstname,ul.lastname ,sl.nombre_vendedor , sl.total , b.quantity , b.art_name , b.price, (b.price * b.quantity), sl.efectivo, sl.credit, sl.cheques, sl.tarjeta, sl.vales, sl.terminacion
FROM sales as sl
JOIN users_local as ul on ul.id_user = sl.id_user 
JOIN bag as b on b.id_sale = sl.id_sale WHERE CAST(sl.register_date as Date) BETWEEN $P{f1} AND $P{f2}]]>
	</queryString>
	<field name="Credito" class="java.lang.Double">
